{
  "workbench.iconTheme": "symbols",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "security.workspace.trust.untrustedFiles": "open",
  "gitlens.hovers.currentLine.over": "line",
  "git.suggestSmartCommit": false,
  "git.confirmSync": false,
  "git.autofetch": true,
  "gitlens.defaultTimeFormat": "HH:mm",
  "gitlens.defaultDateLocale": "pt",
  "gitlens.defaultDateFormat": "DD/MM/YYYY",
  "gitlens.defaultDateShortFormat": "DD/MM",
  "diffEditor.ignoreTrimWhitespace": false,
  "terminal.explorerKind": "external",
  "terminal.integrated.cursorStyle": "line",
  "cSpell.language": "en,pt-BR",
  "explorer.confirmDragAndDrop": false,
  "cSpell.userWords": [
    "csrf",
    "endfor",
    "endforeach",
    "endpush",
    "endsection",
    "Fantasque",
    "imobiliaria",
    "imobiliarias",
    "instagram",
    "isset",
    "laravel",
    "livewire",
    "mysql",
    "opentype",
    "postgres",
    "Roboto",
    "srcset",
    "stylesheet",
    "tbody",
    "thead",
    "urlencode",
    "webp",
    "whatsapp",
    "yesdev",
    "Yesdev",
    "youtube"
  ],
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.fontFamily": "FantasqueSansM Nerd Font Mono",
  "editor.codeActionsOnSave": {},
  "[dockercompose]": {
    "editor.tabSize": 4
  },
  "markdown.preview.fontSize": 12,
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 25,
  "[python]": {
    "editor.formatOnType": true
  },
  "terminal.external.windowsExec": "C:\\Arch\\Arch.exe",
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "addWords": true,
      "scope": "user"
    }
  },
  "extensions.ignoreRecommendations": true,
  "liveServer.settings.donotVerifyTags": true,
  "workbench.colorTheme": "Bearded Theme Arc",
  "editor.stickyScroll.enabled": true,
  "editor.lineHeight": 1.6,
  "symbols.hidesExplorerArrows": false,
  "workbench.startupEditor": "none",
  "editor.fontLigatures": true,
  "editor.renderLineHighlight": "gutter",
  "workbench.editor.labelFormat": "short",
  "explorer.compactFolders": false,
  "breadcrumbs.enabled": false,
  "workbench.activityBar.location": "top",
  "window.customTitleBarVisibility": "auto",
  "editor.minimap.enabled": false,
  "workbench.statusBar.visible": false,
  "editor.semanticHighlighting.enabled": false,
  "explorer.confirmPasteNative": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.autoSave": "afterDelay",
  "terminal.integrated.fontFamily": "FantasqueSansM Nerd Font Mono",
  "terminal.integrated.fontSize": 14,
  "window.commandCenter": false,
  "editor.fontSize": 14,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.tabSize": 4,
  "window.zoomLevel": -1
}
