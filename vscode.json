{
    "workbench.iconTheme": "symbols",
    "bracketPairColorizer.depreciation-notice": false,
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "glassit.alpha": 220,
    "security.workspace.trust.untrustedFiles": "open",
    "gitlens.hovers.currentLine.over": "line",
    "git.suggestSmartCommit": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "npm.keybindingsChangedWarningShown": true,
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.defaultDateLocale": "pt",
    "gitlens.defaultDateFormat": "DD/MM/YYYY",
    "gitlens.defaultDateShortFormat": "DD/MM",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.explorerKind": "external",
    "terminal.integrated.automationProfile.windows": {},
    "terminal.integrated.cursorStyle": "line",
    "cSpell.language": "en,pt-BR",
    "explorer.confirmDragAndDrop": false,
    "cSpell.userWords": [
      "Fantasque",
      "opentype",
      "postgres",
      "Roboto",
      "webp",
      "youtube",
      "whatsapp",
      "instagram",
      "mysql"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "editor.fontFamily": "FantasqueSansMono Nerd Font",
    "editor.codeActionsOnSave": {},
    "[dockercompose]": {
      "editor.tabSize": 4
    },
    "markdown.preview.fontSize": 12,
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 25,
    "[python]": {
      "editor.formatOnType": true
    },
    "terminal.external.windowsExec": "C:\\Arch\\Arch.exe",
    "rainbowBrackets.roundBracketsColor": ["#e6b422", "#c70067", "#fc7482"],
    "rainbowBrackets.squareBracketsColor": ["#33ccff", "#8080ff", "#0073a8"],
    "rainbowBrackets.squigglyBracketsColor": ["#33ccff", "#8080ff", "#0073a8"],
    "cSpell.customDictionaries": {
      "custom-dictionary-user": {
        "name": "custom-dictionary-user",
        "path": "~/.cspell/custom-dictionary-user.txt",
        "addWords": true,
        "scope": "user"
      }
    },
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "liveServer.settings.donotVerifyTags": true,
    "workbench.colorTheme": "Bearded Theme Anthracite",
    "open-php-html-js-in-browser.selectedBrowser": "Opera",
    "editor.stickyScroll.enabled": true,
    "editor.lineHeight": 1.6,
    "editor.fontSize": 12.8,
    "window.zoomLevel": -1,
    "symbols.hidesExplorerArrows": false,
    "workbench.startupEditor": "none",
    "editor.fontLigatures": true,
    "editor.renderLineHighlight": "gutter",
    "workbench.editor.labelFormat": "short",
    "explorer.compactFolders": false,
    "breadcrumbs.enabled": false,
    "workbench.activityBar.location": "top",
    "window.customTitleBarVisibility": "never",
    "editor.minimap.enabled": false,
    "workbench.statusBar.visible": false
  }